{
  "expo": {
    "name": "Flash Cards",
    "slug": "flashCards",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#3C2157"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "br.com.treepixel.flashcard"
    },
    "android": {
      "package": "br.com.treepixel.flashcard"
    },
    "description": "Flash Cards: A React-Native application for Udacity Nano Degree in React",
    "githubUrl": "https://github.com/treepixel/flashCards"
  }
}
